#include <bits/stdc++.h>
using namespace std;
void primality_test (int n){
    if (n==1) cout<<"no.is not prime";
    for (int i=2;i<=sqrt(n);i++){
        if (n%i==0){ 
        cout<<"no.is not prime";
        return;}
    }
    cout<<"no. is prime";
}
int main() {
	int n;cin>>n;
	primality_test(n);
	return 0;
}
//time complexity O(sqrt (n))
